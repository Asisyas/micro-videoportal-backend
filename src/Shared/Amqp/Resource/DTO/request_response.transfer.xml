<?xml version="1.0"?>
<dto xmlns="micro:dto-01"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="micro:dto-01 https://raw.githubusercontent.com/Micro-PHP/dto/master/src/Resource/schema/dto-01.xsd">

    <class name="Amqp\Request">
        <property name="publisher" type="string" required="true" />
        <property name="message" type="mixed" required="true"/>
    </class>

    <class name="Amqp\RpcRequest">
        <property name="publisher" type="string" required="true" />
        <property name="message" type="mixed" required="true"/>
    </class>

    <class name="Amqp\RpcResponse">
        <property name="content" type="mixed" required="true" />
    </class>


    <class name="Amqp\Response">
        <property name="channel_id" type="string" required="true"/>
    </class>

</dto>